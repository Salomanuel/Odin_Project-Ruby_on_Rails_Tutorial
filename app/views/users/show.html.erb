<% provide(:title, @user.name) %>
<div class="row">
	<aside class="col-md-4">
	<section class="user_info">
		<h1> 	
			<%= @user.name %>
			<%= gravatar_for @user %>
		</h1>
	</section>
	</aside>
</div>
<p> <%= @user.email %> 										</p>
<p> created at:		<%= @user.created_at %> </p>
<p> modified at: 	<%= @user.updated_at %> </p>